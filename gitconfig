# put this in the $HOME/.gitconfig file
[user]
	email = louielouie314@gmail.com
	name = Georgy Moshkin
[alias]
	logg = log --graph
	log1 = log --oneline
	log1g = log --oneline --graph
	logg1 = log --graph --oneline
	last = log -1
	loggp = log --graph --pretty='%Cgreen%ar%Creset:%Cblue%h%Creset (%Cred%an%Creset) %s'
	curbranch = rev-parse --abbrev-ref HEAD
	curhash = rev-parse --verify HEAD
[core]
	excludesfile = ~/.gitignore
[push]
	default = simple
